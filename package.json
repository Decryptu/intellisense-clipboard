{
	"name": "intellisense-clipboard",
	"displayName": "IntelliSense Clipboard",
	"description": "Quickly copy TypeScript and JavaScript error messages from the Quick Fix menu to your clipboard",
	"version": "0.1.0",
	"publisher": "Decrypt",
	"icon": "icon.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/Decryptu/intellisense-clipboard"
	},
	"engines": {
		"vscode": "^1.104.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"clipboard",
		"copy",
		"error",
		"diagnostic",
		"typescript",
		"javascript",
		"quick fix"
	],
	"activationEvents": [
		"onLanguage:typescript",
		"onLanguage:javascript",
		"onLanguage:typescriptreact",
		"onLanguage:javascriptreact"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "intellisenseClipboard.copyContent",
				"title": "Copy Error Message"
			},
			{
				"command": "intellisenseClipboard.copyAtCursor",
				"title": "Copy Diagnostic at Cursor"
			}
		],
		"keybindings": [
			{
				"command": "intellisenseClipboard.copyAtCursor",
				"key": "ctrl+alt+c",
				"mac": "cmd+alt+c",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"format": "biome format --write ."
	},
	"devDependencies": {
		"@biomejs/biome": "^2.2.5",
		"@types/node": "^24.7.0",
		"@types/bun": "latest",
		"@types/vscode": "^1.104.0",
		"typescript": "^5.9.3"
	}
}
